<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/blue"
    android:layout_height="match_parent">

    <View
        android:id="@+id/viewBanner"
        android:layout_width="412dp"
        android:layout_height="69dp"
        android:background="@color/orange"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="665dp" />


    <ImageView
        android:id="@+id/imageViewProfile2"
        android:layout_width="112dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.327"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.997"
        app:srcCompat="@drawable/img_4" />

    <ImageView
        android:id="@+id/imageViewStats2"
        android:layout_width="99dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.663"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/img_5" />

    <ImageView
        android:id="@+id/imageViewSettings2"
        android:layout_width="105dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/img_2" />

    <View
        android:id="@+id/view"
        android:layout_width="335dp"
        android:layout_height="536dp"
        android:background="@color/white"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="54dp" />


    <Button
        android:id="@+id/button"
        android:layout_width="114dp"
        android:layout_height="58dp"
        android:backgroundTint="@color/orange"
        android:text="Submit"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="511dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="101dp"
        android:layout_height="62dp"
        app:srcCompat="@drawable/img_3"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="668dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="192dp"
        android:layout_height="51dp"
        android:text="How are you feeling today?"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="82dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="96dp"
        android:layout_height="81dp"
        app:srcCompat="@drawable/img_6"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="154dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="130dp"
        android:layout_height="88dp"
        app:srcCompat="@drawable/img_7"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="152dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="121dp"
        android:layout_height="85dp"
        app:srcCompat="@drawable/img_8"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="154dp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="197dp"
        android:layout_height="123dp"
        android:background="@color/black"
        android:ems="10"
        android:hint="Journal entry"
        android:inputType="text"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="12dp"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="282dp" />

</androidx.constraintlayout.widget.ConstraintLayout>